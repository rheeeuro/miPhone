extends layouts/main
include mixins/phoneBlock

block content
    .companyDetail__info
        img.u-avatar(src=company.imageUrl)
        a(href=company.homepage)
            p=company.name
    .companyDetail__phones
        each item in company.phones
            +phoneBlock({
                id:item.id,
                name:item.name,
                imageUrl:item.imageUrl,
                company:item.company
            })
