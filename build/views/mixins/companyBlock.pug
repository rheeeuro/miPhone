include phoneBlock

mixin companyBlock(company = {})
    .companyBlock
        .companyBlock__img
            img.u-avatar(src=company.imageUrl)
        .companyBlock__name
            a(href=routes.companyDetail(company.id))
                p=company.name
        .companyBlock__phones
            each item in company.phones
                +phoneBlock({
                    id:item.id,
                    name:item.name,
                    imageUrl:item.imageUrl
                })
