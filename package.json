{
  "name": "miphone",
  "version": "1.0.0",
  "description": "Comparing specification of smartphones using database (DB class team project)",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "shx cp -r src/static build/static && shx cp -r src/views build/views",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "buildWindow": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuildWindow": "rd /s /q build",
    "heroku-prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rheeeuro/miPhone.git"
  },
  "author": "Rhee Euro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rheeeuro/miPhone/issues"
  },
  "homepage": "https://github.com/rheeeuro/miPhone#readme",
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.1.2",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flash": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-sass": "^4.13.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "passport-naver": "^1.0.6",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.0",
    "shx": "^0.3.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  }
}
